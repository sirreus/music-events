{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 90,
  "useTabs": false,
  "endOfLine": "auto",
  "importOrder": [
    "^@angular/(.*)$",
    "^(.*)ngrx(.*)$",
    "^(.*)module(.*)$",
    "^(.*)service(.*)$",
    "^(.*)guard(.*)$",
    "^(.*)pipe(.*)$",
    "^rxjs/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "decorators-legacy",
    "[\"decorators\", { \"decoratorsBeforeExport\": true }]"
  ],
  "parser": "typescript"
}
